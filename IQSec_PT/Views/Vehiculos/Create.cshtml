@model IQSec_PT.Models.Vehiculo

@{
    ViewData["Title"] = "Crear";
}

<h1>Crear</h1>

<h4>Ingresa placas</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="placas" class="control-label"></label>
                <input asp-for="placas" class="form-control" />
                <span asp-validation-for="placas" class="text-danger"></span>
            </div>
            <div class="form-group mt-4">
                <input type="submit" value="Dar alta" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div class="mt-3">
    <a asp-action="Index">Regresar al listado </a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
